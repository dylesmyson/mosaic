#!/usr/bin/env python3



from sys import argv

from mosaic.lib import connect



if __name__ == '__main__':

    name = argv[1] if len(argv) > 1 else 'mosaic'

    # `connect` will attempt to establish a **MIDI** connection
    session = connect(name)

    print("Session connected!")
    print("Call `help(session)` for help.")
