#!/usr/bin/env python3



from sys import argv

from lib import *



if __name__ == '__main__':

    filepathstr = str(argv[1]) if len(argv) > 1 else 'data/mid/demo.mid'

    session  = Session()
    print("Session connected!")
    session.load(filepathstr)
    session.play()

    print("Call `help(session)` for help.")
